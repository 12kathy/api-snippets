$ curl -XPOST https://taskrouter.twilio.com/v1/Workspaces/{WorkspaceSid}/Tasks/{TaskSid} \
    --data-urlencode Attributes='{"type":"support"}' \
    -u '{account_sid}:{auth_token}'

$ curl -XPOST https://taskrouter.twilio.com/v1/Workspaces/{WorkspaceSid}/Tasks/{TaskSid} \
    -d "AssignmentStatus=canceled" \
    -d "Reason=waiting too long" \
    -u '{account_sid}:{auth_token}'